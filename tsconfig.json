{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "reactNamespace": "Snabbdom",
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  },
  "exclude": [
    "lib",
    "node_modules",
    "test"
  ]
}